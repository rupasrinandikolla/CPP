#include <stdio.h>
#include <stdlib.h>
#include <math.h>

struct triangle
{
	int a;
	int b;
	int c;
};

typedef struct triangle triangle;
void sort_by_area(triangle* tr, int n) {
	double area[n];
    
    for (int i=0;i<n;i++){
        double a= tr[i].a;
        double b= tr[i].b;
        double c= tr[i].c;
        double s= (a+b+c)/2.0;
        area[i] = s*(s-a)*(s-b)*(s-c);
    }
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n - 1; j++) {
            if (area[j] > area[j + 1]) {
                // Swap areas
                double temp = area[j];
                area[j] = area[j + 1];
                area[j + 1] = temp;

                // Swap triangles
                triangle tmp = tr[j];
                tr[j] = tr[j + 1];
                tr[j + 1] = tmp;
            }
        }
    }
}


int main()
{
	int n;
	scanf("%d", &n);
	triangle *tr = malloc(n * sizeof(triangle));
	for (int i = 0; i < n; i++) {
		scanf("%d%d%d", &tr[i].a, &tr[i].b, &tr[i].c);
	}
	sort_by_area(tr, n);
	for (int i = 0; i < n; i++) {
		printf("%d %d %d\n", tr[i].a, tr[i].b, tr[i].c);
	}
	return 0;
}
